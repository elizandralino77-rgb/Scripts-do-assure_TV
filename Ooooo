	TitleLabel.Text = "Kit Kat Hub"
	TitleLabel.TextColor3 = LoadingConfig.TextColor
	TitleLabel.TextScaled = true
	TitleLabel.Font = Enum.Font.GothamBold
	TitleLabel.ZIndex = 12
	local TitleStroke = Instance.new("UIStroke")
