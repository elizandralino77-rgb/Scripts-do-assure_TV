    "Really red","Hot pink","Pink","Carnation pink","Sunrise","Salmon","Light reddish violet"
}

local remoteSkin = game:GetService("ReplicatedStorage").Remotes:FindFirstChild("ChangeBodyColor")
local skinRGBActive = false
local skinRGBReverseActive = false
local skinSpeed = 10

-- Slider velocidade da pele
RGBSection:AddSlider("Velocidade RGB da Pele", 1, 50, 10, 1, nil, function(v)
    skinSpeed = v
end)

-- Função RGB normal da pele
local function startSkinRGB()
    task.spawn(function()
        while skinRGBActive do
            for _, colorName in ipairs(skinColors) do
                if not skinRGBActive then break end
                remoteSkin:FireServer(colorName)
                task.wait(1 / skinSpeed)
            end
        end
    end)
end

-- Função RGB invertido da pele
local function startSkinRGBReverse()
    task.spawn(function()
        while skinRGBReverseActive do
            for i = #skinColors, 1, -1 do
                if not skinRGBReverseActive then break end
                remoteSkin:FireServer(skinColors[i])
                task.wait(1 / skinSpeed)
            end
        end
    end)
end

-- Toggle RGB normal da pele
RGBSection:AddToggle("RGB DA PELE (Normal) ON/OFF", false, nil, function(state)
    skinRGBActive = state
    if state then startSkinRGB() end
end)

-- Toggle RGB invertido da pele
RGBSection:AddToggle("RGB DA PELE (Invertido) ON/OFF", false, nil, function(state)
    skinRGBReverseActive = state
    if state then startSkinRGBReverse() end
end)





----------------------------------------------------------
-- ABA TELA
----------------------------------------------------------
local TelaTab = Window:AddTab("Tela", 6) -- ordem = 6
local TelaSection = TelaTab:AddSection("Efeitos Visuais")

-- Função auxiliar para executar scripts de forma segura
local function executarScript(scriptString)
    local func, err = loadstring(scriptString)
    if func then
        func()
    else
        warn("Erro ao carregar o script: "..err)
    end
end

-- Botão Cinematográfico
TelaSection:AddButton("Cinematográfico", function()
    executarScript([[
local Lighting = game:GetService("Lighting")

local function getOrCreateEffect(class,name)
    local effect = Lighting:FindFirstChild(name)
    if not effect then
        effect = Instance.new(class)
        effect.Name = name
        effect.Parent = Lighting
    end
    return effect
end

local sun = getOrCreateEffect("SunRaysEffect","CinematicSunRays")
sun.Intensity = 0.12
sun.Spread = 0.8

Settings().Rendering.TextureQuality = Enum.TextureQuality.High
Settings().Rendering.GraphicsMode = Enum.GraphicsMode.DirectX11

print("Modo Cinematográfico ativado!")
    ]])
end)

-- Botão Terror
TelaSection:AddButton("Terror", function()
    executarScript([[
local Lighting = game:GetService("Lighting")

local function getOrCreateEffect(class,name)
    local effect = Lighting:FindFirstChild(name)
    if not effect then
        effect = Instance.new(class)
        effect.Name = name
        effect.Parent = Lighting
    end
    return effect
end

Lighting.Technology = Enum.Technology.ShadowMap
Lighting.GlobalShadows = true
Lighting.Ambient = Color3.fromRGB(120,130,120)
Lighting.OutdoorAmbient = Color3.fromRGB(150,160,150)
Lighting.Brightness = 1.0
Lighting.TimeOfDay = "18:00:00"

local cc = getOrCreateEffect("ColorCorrectionEffect","TerrorVisualColor")
cc.Saturation = -0.5
cc.Contrast = 0.4
cc.TintColor = Color3.fromRGB(150,150,180)

Lighting.FogStart = 20
Lighting.FogEnd = 80
Lighting.FogColor = Color3.fromRGB(100,100,100)

local bloom = getOrCreateEffect("BloomEffect","TerrorVisualBloom")
bloom.Intensity = 0.3
bloom.Size = 10
bloom.Threshold = 1.0

local blur = getOrCreateEffect("BlurEffect","TerrorVisualBlur")
blur.Size = 2.0

print("Modo Terror ativado!")
    ]])
end)



----------------------------------------------------------
-- ABA ESPS
----------------------------------------------------------
local ESPS_Tab = Window:AddTab("ESPS", 8) -- ordem = 8
local ESPS_Section = ESPS_Tab:AddSection("ESP Controls")

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local LocalPlayer = Players.LocalPlayer
local Workspace = game:GetService("Workspace")

-- Estado global
local NAME_ESP_ENABLED = false
local BOX_ESP_ENABLED = false
local RAINBOW_MODE = false
local SELECTED_COLOR = Color3.fromRGB(255,255,255)
local FONT_SIZE = 18

-- Tables para ESPs
local NameESPs = {}
local BoxESPs = {}

-- Lista de cores
local cores = {
    ["Rainbow"] = "rainbow",
    ["Vermelho"] = Color3.fromRGB(255,0,0),
    ["Azul"] = Color3.fromRGB(0,0,255),
    ["Verde"] = Color3.fromRGB(0,255,0),
    ["Roxo"] = Color3.fromRGB(170,0,255),
    ["Amarelo"] = Color3.fromRGB(255,255,0),
    ["Branco"] = Color3.fromRGB(255,255,255),
    ["Preto"] = Color3.fromRGB(0,0,0),
    ["Ciano"] = Color3.fromRGB(0,255,255),
    ["Laranja"] = Color3.fromRGB(255,140,0),
    ["Rosa"] = Color3.fromRGB(255,105,180),
    ["Marrom"] = Color3.fromRGB(150,75,0),
    ["Cinza"] = Color3.fromRGB(128,128,128),
    ["Dourado"] = Color3.fromRGB(212,175,55),
    ["Verde Escuro"] = Color3.fromRGB(0,100,0),
    ["Azul Claro"] = Color3.fromRGB(135,206,235),
    ["Magenta"] = Color3.fromRGB(255,0,255)
}
local colorSelectedName = "Branco"

-- Função para pegar idade da conta
local function GetAccountAgeDays(player)
    return tostring(player.AccountAge or 0)
end

-- Funções para Name ESP
local function CreateNameESP(player)
    if not player.Character or NameESPs[player] then return end
    local head = player.Character:FindFirstChild("Head")
    if not head then return end

    local bill = Instance.new("BillboardGui")
    bill.Name = "Spectra_NameESP"
    bill.Adornee = head
    bill.Size = UDim2.new(0,200,0,50)
    bill.StudsOffset = Vector3.new(0,2.5,0)
    bill.AlwaysOnTop = true

    local label = Instance.new("TextLabel")
    label.Size = UDim2.new(1,0,1,0)
    label.BackgroundTransparency = 1
    label.TextScaled = false
    label.Font = Enum.Font.GothamBold
    label.TextSize = FONT_SIZE
    label.TextStrokeTransparency = 0.2
    label.Text = player.Name.." | "..GetAccountAgeDays(player).." dias"
    if not RAINBOW_MODE then label.TextColor3 = SELECTED_COLOR end
    label.Parent = bill

    bill.Parent = head
    NameESPs[player] = {Bill=bill, Label=label}

    player.CharacterAdded:Connect(function()
        task.wait(0.8)
        if NAME_ESP_ENABLED then
            if NameESPs[player] and NameESPs[player].Bill then NameESPs[player].Bill:Destroy() end
            NameESPs[player] = nil
            CreateNameESP(player)
        end
    end)
end

local function RemoveNameESP(player)
    if NameESPs[player] then
        pcall(function() NameESPs[player].Bill:Destroy() end)
        NameESPs[player] = nil
    end
end

-- Funções para Box ESP
local function CreateBoxESP(player)
    if not player.Character or BoxESPs[player] then return end
    local model = player.Character
    local highlight = Instance.new("Highlight")
    highlight.Adornee = model
    highlight.FillTransparency = 1
    highlight.OutlineTransparency = 0
    highlight.OutlineColor = SELECTED_COLOR
    highlight.Parent = Workspace
    BoxESPs[player] = highlight

    player.CharacterAdded:Connect(function()
        task.wait(0.8)
        if BOX_ESP_ENABLED then
            RemoveBoxESP(player)
            CreateBoxESP(player)
        end
    end)
end

local function RemoveBoxESP(player)
    if BoxESPs[player] then
        pcall(function() BoxESPs[player]:Destroy() end)
        BoxESPs[player] = nil
    end
end

-- Funções Enable/Disable
local function EnableNameESPs()
    for _,plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then CreateNameESP(plr) end
    end
end
local function DisableNameESPs()
    for plr,_ in pairs(NameESPs) do RemoveNameESP(plr) end
end
local function EnableBoxESPs()
    for _,plr in pairs(Players:GetPlayers()) do
        if plr ~= LocalPlayer then CreateBoxESP(plr) end
    end
end
local function DisableBoxESPs()
    for plr,_ in pairs(BoxESPs) do RemoveBoxESP(plr) end
end

-- RenderStepped update
RunService.RenderStepped:Connect(function()
    local hue = (tick()%5)/5
    for plr,data in pairs(NameESPs) do
        if data and data.Label then
            data.Label.Text = plr.Name.." | "..GetAccountAgeDays(plr).." dias"
            data.Label.TextSize = FONT_SIZE
            if RAINBOW_MODE then
                data.Label.TextColor3 = Color3.fromHSV(hue,1,1)
            else
                data.Label.TextColor3 = SELECTED_COLOR
            end
        end
    end
    for plr,highlight in pairs(BoxESPs) do
        if highlight then
            if RAINBOW_MODE then
                highlight.OutlineColor = Color3.fromHSV(hue,1,1)
            else
                highlight.OutlineColor = SELECTED_COLOR
            end
        end
    end
end)

-- GUI Controls Zov UI
ESPS_Section:AddToggle("ESP Nome ON/OFF", function(state)
    NAME_ESP_ENABLED = state
    if state then EnableNameESPs() else DisableNameESPs() end
end)

ESPS_Section:AddToggle("ESP Box ON/OFF", function(state)
    BOX_ESP_ENABLED = state
    if state then EnableBoxESPs() else DisableBoxESPs() end
end)

local colorOptions = {}
for k,_ in pairs(cores) do table.insert(colorOptions,k) end
table.sort(colorOptions)

ESPS_Section:AddDropdown("Escolher Cor", colorOptions, function(selected)
    colorSelectedName = selected
    if colorSelectedName == "Rainbow" then
        RAINBOW_MODE = true
    else
        RAINBOW_MODE = false
        SELECTED_COLOR = cores[colorSelectedName] or Color3.fromRGB(255,255,255)
        for _,data in pairs(NameESPs) do
            if data and data.Label then data.Label.TextColor3 = SELECTED_COLOR end
        end
        for _,highlight in pairs(BoxESPs) do
            if highlight then highlight.OutlineColor = SELECTED_COLOR end
        end
    end
end)

ESPS_Section:AddSlider("Tamanho do Nome + Dias", 10, 40, FONT_SIZE, function(size)
    FONT_SIZE = size
    for _,data in pairs(NameESPs) do
        if data and data.Label then data.Label.TextSize = FONT_SIZE end
    end
end)

print("Spectra ESP (Zov UI) finalizado!")



