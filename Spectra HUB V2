--// Loading Zov UI Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/egor2078f/Zov-UI/refs/heads/main/zov.lua"))() 

--// Creating window
local Window = Library:CreateWindow("Spectra HUB / V2", Enum.KeyCode.RightControl)

----------------------------------------------------------
-- VARIÁVEIS GLOBAIS
----------------------------------------------------------
local Players = game:GetService("Players")
local plr = Players.LocalPlayer

local currentSpeed = 16
local currentJump = 50
local currentGrav = workspace.Gravity

local infinityJumpEnabled = false
local noclipEnabled = false

local selectedPlayer = nil
local killing = false
local noclipKillEnabled = false
local viewEnabled = false
local followOscillation = false

local FRONT_OFFSET = 0.35
local BACK_OFFSET = -3.5
local SPEED = 0.75

-- Função para pegar humanoid
local function getHumanoid()
    if plr.Character then
        return plr.Character:FindFirstChildOfClass("Humanoid")
    end
end

-- Atualizar valores ao respawn
plr.CharacterAdded:Connect(function(char)
    task.wait(0.3)
    local hum = char:WaitForChild("Humanoid")
    hum.UseJumpPower = true
    hum.JumpPower = currentJump
    hum.WalkSpeed = currentSpeed
    workspace.Gravity = currentGrav
end)

----------------------------------------------------------
-- ABA PLAYER
----------------------------------------------------------
local PlayerTab = Window:AddTab("Player", 1)
local PlayerSection = PlayerTab:AddSection("Configurações do Personagem")

-- Sliders
local walkSpeedValue = currentSpeed
PlayerSection:AddSlider("WalkSpeed", 16, 500, currentSpeed, 1, nil, function(value)
    walkSpeedValue = value
    local hum = getHumanoid()
    if hum then hum.WalkSpeed = value end
end)

local jumpPowerValue = currentJump
PlayerSection:AddSlider("JumpPower", 50, 500, currentJump, 1, nil, function(value)
    jumpPowerValue = value
    local hum = getHumanoid()
    if hum then
        hum.UseJumpPower = true
        hum.JumpPower = value
    end
end)

local gravityValue = currentGrav
PlayerSection:AddSlider("Gravidade", 50, 500, currentGrav, 1, nil, function(value)
    gravityValue = value
    workspace.Gravity = value
end)

-- Botão Resetar
PlayerSection:AddButton("Resetar", function()
    walkSpeedValue = 16
    jumpPowerValue = 50
    gravityValue = 196

    local hum = getHumanoid()
    if hum then
        hum.WalkSpeed = 16
        hum.UseJumpPower = true
        hum.JumpPower = 50
    end

    workspace.Gravity = 196

    PlayerSection:GetSlider("WalkSpeed"):Set(16)
    PlayerSection:GetSlider("JumpPower"):Set(50)
    PlayerSection:GetSlider("Gravidade"):Set(196)

    Library:Notify("Player", "Valores resetados!", 2)
end)

-- Botões de Scripts Externos
local Scripts = {
    ["Shift Lock"] = "https://rawscripts.net/raw/Universal-Script-Shift-lock-64302",
    ["Fly"] = "https://rawscripts.net/raw/Universal-Script-Gui-Fly-v3-37111",
    ["Infinite Yield"] = "https://rawscripts.net/raw/Universal-Script-Infinite-yield-port-Vxeze-72354",
    ["Emotes"] = "https://rawscripts.net/raw/Universal-Script-7yd7-I-Emote-Script-48024"
}

for name, url in pairs(Scripts) do
    PlayerSection:AddButton(name, function()
        loadstring(game:HttpGet(url))()
    end)
end

-- Infinity Jump Toggle
PlayerSection:AddToggle("Infinity Jump", false, nil, function(state)
    infinityJumpEnabled = state
end)

game:GetService("UserInputService").JumpRequest:Connect(function()
    if infinityJumpEnabled then
        local hum = getHumanoid()
        if hum then
            hum:ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

-- NoClip Toggle
PlayerSection:AddToggle("NoClip", false, nil, function(state)
    noclipEnabled = state
end)

game:GetService("RunService").Stepped:Connect(function()
    if noclipEnabled and plr.Character then
        for _, part in ipairs(plr.Character:GetDescendants()) do
            if part:IsA("BasePart") then
                part.CanCollide = false
            end
        end
    end
end)

----------------------------------------------------------
-- ABA TROLL
----------------------------------------------------------
local TrollTab = Window:AddTab("Troll", 2)
local TrollSection = TrollTab:AddSection("Troll Features")

-- Função para obter lista de jogadores
local function getPlayerNames()
    local names = {}
    for _, p in ipairs(Players:GetPlayers()) do
        if p ~= plr then
            table.insert(names, p.Name)
        end
    end
    return names
end

-- Criar dropdown inicial
local playerDropdown
local function createDropdown()
    if playerDropdown then
        playerDropdown:Destroy()
    end

    playerDropdown = TrollSection:AddDropdown("Escolher Player", getPlayerNames(), 1, nil, function(value)
        selectedPlayer = value
    end)

    selectedPlayer = nil
end

createDropdown()

-- Botão Atualizar Lista
TrollSection:AddButton("Atualizar Lista", function()
    createDropdown()
end)

-- Botão Kill
TrollSection:AddButton("Kill demorado e chato kkkk", function()
    if not selectedPlayer then return end
    killing = true
    noclipKillEnabled = true

    -- PickingTools
    local args = { [1]="PickingTools",[2]="Couch" }
    game:GetService("ReplicatedStorage").RE:FindFirstChild("1Too1l"):InvokeServer(unpack(args))

    -- Teleporte rápido
    task.spawn(function()
        while killing do
            local target = Players:FindFirstChild(selectedPlayer)
            local char = plr.Character
            local hum = char and char:FindFirstChild("HumanoidRootPart")
            if target and target.Character and hum then
                local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
                if targetHRP then
                    local offsets = {
                        Vector3.new(0,5,0), Vector3.new(0,-5,0),
                        Vector3.new(5,0,0), Vector3.new(-5,0,0),
                        Vector3.new(0,0,5), Vector3.new(0,0,-5),
                        Vector3.new(5,0,5), Vector3.new(-5,0,5),
                        Vector3.new(5,0,-5), Vector3.new(-5,0,-5)
                    }
                    for _, offset in ipairs(offsets) do
                        if not killing then break end
                        hum.CFrame = CFrame.new(targetHRP.Position + offset, targetHRP.Position)
                        task.wait(0.01)
                    end
                end
            end
            task.wait(0.01)
        end
    end)

    -- NoClip Kill
    task.spawn(function()
        while noclipKillEnabled do
            local char = plr.Character
            if char then
                for _, part in ipairs(char:GetDescendants()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
            end
            task.wait(0.01)
        end
    end)
end)

-- Botão Stop Kill
TrollSection:AddButton("Stop Kill", function()
    killing = false
    noclipKillEnabled = false
    if viewEnabled and plr.Character then
        workspace.CurrentCamera.CameraSubject = plr.Character:FindFirstChildOfClass("Humanoid")
        workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
    end
end)

-- Toggle Visualizar Player
TrollSection:AddToggle("Visualizar Player", false, nil, function(state)
    viewEnabled = state
    local target = Players:FindFirstChild(selectedPlayer)
    if viewEnabled then
        if target and target.Character then
            workspace.CurrentCamera.CameraSubject = target.Character:FindFirstChildOfClass("Humanoid")
            workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        end
    else
        if plr.Character then
            workspace.CurrentCamera.CameraSubject = plr.Character:FindFirstChildOfClass("Humanoid")
            workspace.CurrentCamera.CameraType = Enum.CameraType.Custom
        end
    end
end)

-- Botão Teleportar
TrollSection:AddButton("Teleportar para o Jogador", function()
    if not selectedPlayer then return end
    local target = Players:FindFirstChild(selectedPlayer)
    if target and target.Character then
        local targetHRP = target.Character:FindFirstChild("HumanoidRootPart")
        local char = plr.Character
        local hum = char and char:FindFirstChild("HumanoidRootPart")
        if targetHRP and hum then
            hum.CFrame = targetHRP.CFrame
        end
    end
end)

-- Toggle Seguir Frente/Trás
TrollSection:AddToggle("Seguir Frente/Trás no Player (ON/OFF)", false, nil, function(state)
    followOscillation = state
    if state then
        local target = Players:FindFirstChild(selectedPlayer)
        if target and target.Character then
            local hrp = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
            local thrp = target.Character:FindFirstChild("HumanoidRootPart")
            if hrp and thrp then
                hrp.CFrame = thrp.CFrame * CFrame.new(0,0,-3.5)
            end
        end
    end
end)

-- Loop Seguir Player
task.spawn(function()
    while task.wait() do
        if followOscillation and selectedPlayer then
            local target = Players:FindFirstChild(selectedPlayer)
            if not target then continue end
            local targetHRP = target.Character and target.Character:FindFirstChild("HumanoidRootPart")
            local myHRP = plr.Character and plr.Character:FindFirstChild("HumanoidRootPart")
            if not targetHRP or not myHRP then continue end
            myHRP.CFrame = myHRP.CFrame:Lerp(targetHRP.CFrame * CFrame.new(0,0,-0.35), 0.75)
            task.wait(0.35)
            myHRP.CFrame = myHRP.CFrame:Lerp(targetHRP.CFrame * CFrame.new(0,0,3.5), 0.75)
            task.wait(0.35)
        end
    end
end)

-- Botão Skybox
TrollSection:AddButton("Skybox", function()
    loadstring(game:HttpGet("https://rawscripts.net/raw/Brookhaven-RP-Fe-Brookhaven-skybox-script-73292"))()
end)

-- Notificação inicial
Library:Notify("Bem-vindo", "UI carregada com sucesso!", 3)



