fun√ß√£o local temCouch()
    caractere local = plr.Character
    local bp = plr:FindFirstChildOfClass("Mochila")
    Se char e char:FindFirstChild("Couch") ent√£o retorne verdadeiro fim
    se bp e bp:FindFirstChild("Couch") ent√£o retorne verdadeiro fim
    retornar falso
fim

fun√ß√£o local pickCouch()
    pcall(fun√ß√£o()
        ReplicatedStorage.RE:FindFirstChild("1Too1l")
            :InvokeServer("PickingTools", "Couch")
    fim)
fim

fun√ß√£o local equipCouch()
    caractere local = plr.Character
    local hum = char e char:FindFirstChildOfClass("Humanoid")
    Se n√£o houver zumbido, retorne ao fim.

    ferramenta local =
        (char e char:FindFirstChild("Couch")) ou
        (plr.Backpack e plr.Backpack:FindFirstChild("Couch"))

    se ferramenta ent√£o
        hum:EquipTool(ferramenta)
    fim
fim

