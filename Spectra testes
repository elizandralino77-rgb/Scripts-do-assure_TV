--==============================
-- SISTEMA LOCAL
--==============================
local SpinPlayers = {}
local FrozenPlayers = {}
local CaixaPlayers = {}

fun√ß√£o local KillLocal() local hum=plr.Character e plr.Character:FindFirstChildOfClass("Humanoid"); se hum ent√£o hum.Health=0 fim fim
fun√ß√£o local FlingLocal() local hrp=plr.Character e plr.Character:FindFirstChild("HumanoidRootPart"); se hrp ent√£o hrp.Velocity=Vector3.new(0,2000,0) fim fim
fun√ß√£o local SitLocal() local hum=plr.Character e plr.Character:FindFirstChildOfClass("Humanoid"); se hum ent√£o hum.Sit=true fim fim
fun√ß√£o local FrozenLocal() local hum=plr.Character e plr.Character:FindFirstChildOfClass("Humanoid"); se hum ent√£o hum.PlatformStand=true; FrozenPlayers[plr.Name]=true fim fim
fun√ß√£o local UnFrozenLocal() local hum=plr.Character e plr.Character:FindFirstChildOfClass("Humanoid"); se hum ent√£o hum.PlatformStand=false; FrozenPlayers[plr.Name]=nil fim fim
fun√ß√£o local SpinLocal() SpinPlayers[plr.Name]=true fim
fun√ß√£o local UnSpinLocal() SpinPlayers[plr.Name]=nil fim
fun√ß√£o local CaixaLocal()
    caractere local=plr.Character
    se char e n√£o CaixaPlayers[plr.Name] ent√£o
        local hrp=char:FindFirstChild("HumanoidRootPart")
        se hrp ent√£o
            caixa local=Instance.new("Modelo")
            caixa.Name="CaixaAdmin"
            tamanho local=Vector3.novo(6,6,6)
            local pos=hrp.Position
            para x=-1,1 fa√ßa para y=-1,1 fa√ßa para z=-1,1 fa√ßa
                se x==0 e y==0 e z==0 ent√£o continue fim
                local p=Instance.new("Part")
                p.Size=size/3 p.Anchored=true p.CanCollide=true p.Color=Color3.new(0,0,0)
                p.Material=Enum.Material.Neon
                p.CFrame=CFrame.new(pos+Vector3.new(x*2,y*2,z*2))
                p.Parent=caixa
            fim fim fim
            caixa.Pai=Espa√ßo de trabalho
            CaixaPlayers[plr.Name]=caixa
        fim
    fim
fim
fun√ß√£o local UnCaixaLocal() local c=CaixaPlayers[plr.Name]; se c ent√£o c:Destroy(); CaixaPlayers[plr.Name]=nil fim fim

--======================= =======
-- OUVINTE DO CHAT
--==============================
fun√ß√£o local OnMessageReceived(mensagem)
    texto local = string.lower(mensagem.Texto)
    cmd local, alvo = texto:match("^;(.-)%s+(.+)$")
    se cmd e alvo e string.lower(alvo)==string.lower(plr.Name) ent√£o
        se cmd="kill" ent√£o KillLocal()
        sen√£o se cmd="fling" ent√£o FlingLocal()
        sen√£o se cmd="sentar" ent√£o SitLocal()
        sen√£o se cmd="congelado" ent√£o CongeladoLocal()
        sen√£o se cmd="unfrozen" ent√£o UnFrozenLocal()
        sen√£o se cmd="spin" ent√£o SpinLocal()
        sen√£o se cmd="unspin" ent√£o UnSpinLocal()
        elseif cmd=="caixa" ent√£o CaixaLocal()
        sen√£o se cmd="uncaixa" ent√£o UnCaixaLocal() fim
    fim
fim

para _, canal em pares(TextChatService.TextChannels:GetChildren()) fa√ßa
    canal.MensagemRecebida:Conectar(AoReceberMensagem)
fim
