TrollTab:AdicionarAltern√¢ncia({
    Name = "Kill demorado e chato üòà",
    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        killEnabled = estado

        -- DESLIGAR TUDO
        se n√£o especificar ent√£o
            Se teleportThread ent√£o task.cancel(teleportThread) teleportThread = nil fim
            Se noclipThread ent√£o task.cancel(noclipThread) noclipThread = nil fim
            Se monitorThread ent√£o task.cancel(monitorThread) monitorThread = nil fim
            Se jumpThread ent√£o task.cancel(jumpThread) jumpThread = nil fim
            se respawnConn ent√£o respawnConn:Desconectar() respawnConn = nil fim
            retornar
        fim

        Se n√£o houver jogador selecionado, retorne ao final.

        -- MONITORAR SOF√Å (pegar automaticamente)
        monitorThread = task.spawn(function()
            enquanto killEnabled fa√ßa
                se n√£o hasCouch() ent√£o
                    escolherSof√°()
                    tarefa.esperar(0.15)
                fim
                equiparCouch()
                tarefa.esperar(0.2)
            fim
        fim)

        -- AO MORRER
        respawnConn = plr.CharacterAdded:Connect(function()
            se killEnabled ent√£o
                tarefa.esperar(0.3)
                escolherSof√°()
            fim
        fim)

        -- TELEPORT CA√ìTICO (MODO N√ÅRNIA üåå)
        teleportThread = task.spawn(function()
            enquanto killEnabled fa√ßa
                alvo local = Jogadores:EncontrarPrimeiroFilho(jogadorSelecionado)
                local hrp = plr.Character e plr.Character:FindFirstChild("HumanoidRootPart")
                local thrp = target and target.Character and target.Character:FindFirstChild("HumanoidRootPart")

                se hrp e thrp ent√£o
                    para i = 1, 60 fa√ßa
                        se n√£o killEnabled ent√£o interrompa o processo
                        diret√≥rio local = Vector3.novo(
                            math.random(-100.100)/100*0.001, -- dist√¢ncia m√≠nima de 1mm
                            math.random(-100,100)/100*0.001,
                            math.random(-100,100)/100*0.001
                        )
                        hrp.CFrame = CFrame.lookAt(thrp.Position + dir, thrp.Position)
                    fim
                fim
                tarefa.esperar(0.001)
            fim
        fim)

        -- NOCLIP FORTE
        noclipThread = task.spawn(function()
            enquanto killEnabled fa√ßa
                caractere local = plr.Character
                se char ent√£o
                    para _, p em ipairs(char:GetDescendants()) fa√ßa
                        se p:IsA("BasePart") ent√£o
                            p.CanCollide = falso
                        fim
                    fim
                fim
                tarefa.esperar()
            fim
        fim)

        -- üî• PULO INFINITO (PC, celular, tablet)
        jumpThread = task.spawn(function()
            enquanto killEnabled fa√ßa
                caractere local = plr.Character
                local hum = char e char:FindFirstChildOfClass("Humanoid")
                se zumbir ent√£o
                    hum.Jump = verdadeiro
                    --Dispara entrada para todos os dispositivos
                    pcall(fun√ß√£o()
                        UserInputService:InputBegan({UserInputType = Enum.UserInputType.Keyboard, KeyCode = Enum.KeyCode.Space}, falso)
                        UserInputService:InputBegan({UserInputType = Enum.UserInputType.Touch}, falso)
                    fim)
                fim
                tarefa.esperar(0.15)
            fim
        fim)
    fim
})


