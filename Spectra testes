local antiSitEnabled = false
anti-SitConn local

PlayerTab:AdicionarAltern√¢ncia({
    Nome = "Anti-Sentado",
    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        antiSitEnabled = estado

        fun√ß√£o local aplicar(caractere)
            local hum = character:FindFirstChildOfClass("Humanoid")
            Se n√£o houver zumbido, retorne ao fim.

            hum.Sit = falso
            hum:SetStateEnabled(Enum.HumanoidStateType.Seated, false)

            se antiSitConn ent√£o
                antiSitConn:Desconectar()
            fim

            antiSitConn = hum.Seated:Connect(function(sentar)
                se sentar ent√£o
                    hum.Sit = falso
                    hum:ChangeState(Enum.HumanoidStateType.GettingUp)
                fim
            fim)
        fim

        se estado ent√£o
            se LocalPlayer.Character ent√£o
                aplicar(LocalPlayer.Character)
            fim

            JogadorLocal.PersonagemAdicionado:Conectar(fun√ß√£o(char)
                se antiSitEnabled ent√£o
                    aplicar(caractere)
                fim
            fim)
        outro
            se antiSitConn ent√£o
                antiSitConn:Desconectar()
                antiSitConn = nulo
            fim

            se LocalPlayer.Character ent√£o
                local hum = LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                se zumbir ent√£o
                    hum:SetStateEnabled(Enum.HumanoidStateType.Seated, true)
                fim
            fim
        fim
    fim
})

