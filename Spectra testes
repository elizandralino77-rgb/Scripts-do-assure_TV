fun√ß√£o local desconectarTudo()
    para _,c em ipairs(Connections) fa√ßa pcall(function() c:Disconnect() fim) fim
    tabela.limpar(Conex√µes)
fim

fun√ß√£o local getChar() retorna LocalPlayer.Character fim
fun√ß√£o local getHRP() local c=getChar() retorna c e c:FindFirstChild("HumanoidRootPart") fim
fun√ß√£o local getHum() local c=getChar() retorna c e c:FindFirstChildOfClass("Humanoid") fim
fun√ß√£o local restorePosition()
    local hrp = getHRP()
    se hrp e lastSafeCFrame ent√£o
        hrp.AssemblyLinearVelocity=Vector3.zero
        hrp.AssemblyAngularVelocity=Vector3.zero
        hrp.CFrame=lastSafeCFrame+Vector3.new(0,5,0)
    fim
fim

Aba de prote√ß√£o:AdicionarAltern√¢ncia({
