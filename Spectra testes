TrollTab:AdicionarAltern√¢ncia({
    Nome = "Teleporte Troll (Ligado/Desligado)",
    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        ativado = estado

        se houver conex√£o ent√£o
            conex√£o:Desconectar()
            conex√£o = nula
        fim

        Se n√£o houver estado ou jogador n√£o selecionado, retorne ao fim.

        timeOffset = 0

        conex√£o = RunService.Heartbeat:Connect(function(dt)
            Se n√£o estiver habilitado, retorne ao fim.

            alvo local = Jogadores:EncontrarPrimeiroFilho(jogadorSelecionado)
            Se n√£o for um alvo ou n√£o for um personagem, retorne.

            local thrp = target.Character:FindFirstChild("HumanoidRootPart")
            caractere local = plr.Character
            local hrp = char e char:FindFirstChild("HumanoidRootPart")

            se n√£o (thrp e hrp) ent√£o retorne fim

            -- TEMPO (slider atua aqui)
            timeOffset += dt * (movementSpeed ‚Äã‚Äã* 0.3)

            -- FRENTE ‚Üî TR√ÅS
            local baseDistance = 4.5 -- mais perto do jogador
            oscila√ß√£o local = math.sin(deslocamento de tempo) * 3

            -- CIMA ‚Üï BAIXO (bem leve)
            local verticalOffset = math.sin(timeOffset * 2) * 0.6

            -- POSI√á√ÉO FINAL
            local targetPos =
                thrp.Posi√ß√£o
                - thrp.CFrame.LookVector * (baseDistance + oscila√ß√£o)
                + Vector3.new(0, verticalOffset, 0)

            -- TREMIDA VISUAL
            agita√ß√£o local = Vector3.new(
                math.random(-10,10) / 70,
                math.random(-10,10) / 70,
                0
            )

            -- APLICAR TELEPORTE
            hrp.CFrame = CFrame.lookAt(
                targetPos + agitar,
                thrp.Posi√ß√£o
            )
        fim)
    fim
})






