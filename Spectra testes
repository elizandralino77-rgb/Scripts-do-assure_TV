                Se doorData.door e doorData.door.Parent ent√£o
                    doorData.door.Transparency = doorData.originalTransparency
                    portaData.door.CanCollide = portaData.originalCanCollide
                    doorData.door.CastShadow = doorData.originalCastShadow
                    para _, filho em ipairs(doorData.door:GetChildren()) fa√ßa
                        Se filho:IsA("BasePart") ent√£o filho.Transpar√™ncia=0; filho.PodeColidir=true
                        sen√£o se filho:IsA("SurfaceGui") ou filho:IsA("BillboardGui") ent√£o filho.Habilitado = verdadeiro fim
                    fim
                fim
            fim
