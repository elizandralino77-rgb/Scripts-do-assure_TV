    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        se n√£o _G.hiddenDoors ent√£o _G.hiddenDoors = {} fim
        se estado ent√£o
            _G.hiddenDoors = {}
            para _, obj em ipairs(workspace:GetDescendants()) fa√ßa
                se obj:IsA("BasePart") e obj.Name:lower():find("door") ent√£o
                    local doorData = {door=obj, originalTransparency=obj.Transparency, originalCanCollide=obj.CanCollide, originalCastShadow=obj.CastShadow}
                    obj.Transpar√™ncia = 1
                    obj.CanCollide = false
                    obj.CastShadow = falso
                    para _, filho em ipairs(obj:GetChildren()) fa√ßa
                        Se filho:IsA("BasePart") ent√£o filho.Transpar√™ncia = 1; filho.PodeColidir = falso
                        sen√£o se filho:IsA("SurfaceGui") ou filho:IsA("BillboardGui") ent√£o filho.Habilitado = falso fim
                    fim
                    tabela.inserir(_G.portasocultas, dadosdaporta)
                fim
            fim
            print("‚ö° " .. #_G.hiddenDoors .. " portas escondidas!")
        outro
