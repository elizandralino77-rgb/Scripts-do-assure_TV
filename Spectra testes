--// ESP COMPLETO
PlayerTab:AdicionarSe√ß√£o({ "ESP" })

local ESPEnabled = false
local ESPColorMode = "RGB"
local ESPObjects = {}
local ESPConnections = {}

PlayerTab:AdicionarMenuSuspenso({
    Nome = "ESP Color",
    Padr√£o = "RGB",
    Op√ß√µes = {
        "RGB","Branco","Preto","Vermelho",
        "Verde","Azul","Amarelo","Rosa","Roxo"
    },
    Callback = fun√ß√£o(v)
        ESPColorMode = v
    fim
})

fun√ß√£o local getESPColor()
    se ESPColorMode == "RGB" ent√£o
        retornar Color3.fromHSV((tick()%5)/5,1,1)
    sen√£o se ESPColorMode == "Branco" ent√£o
        retornar Color3.fromRGB(255,255,255)
    elseif ESPColorMode == "Preto" ent√£o
        retornar Color3.fromRGB(0,0,0)
    elseif ESPColorMode == "Vermelho" ent√£o
        retornar Color3.fromRGB(255,0,0)
    sen√£o se ESPColorMode == "Verde" ent√£o
        retornar Color3.fromRGB(0,255,0)
    sen√£o se ESPColorMode == "Azul" ent√£o
        retornar Color3.fromRGB(0,170,255)
    elseif ESPColorMode == "Amarelo" ent√£o
        retornar Color3.fromRGB(255,255,0)
    sen√£o se ESPColorMode == "Rosa" ent√£o
        retornar Color3.fromRGB(255,105,180)
    sen√£o se ESPColorMode == "Roxo" ent√£o
        retornar Color3.fromRGB(128,0,128)
    fim
    retornar Color3.new(1,1,1)
fim

fun√ß√£o local RemoveESP(jogador)
    se ESPObjects[player] ent√£o
        ESPObjects[player]:Destruir()
        ESPObjects[player] = nulo
    fim
fim

fun√ß√£o local CreateESP(jogador)
    Se jogador == JogadorLocal, ent√£o retorne fim.
    Se ESPEnabled n√£o estiver habilitado, retorne o fim.
    Se n√£o player.Character, retorne end

    local head = player.Character:FindFirstChild("Head")
    Se n√£o for o cabe√ßalho, retorne ao fim.

    RemoverESP(player)

    gui local = Instance.new("BillboardGui")
    gui.Name = "ESP"
    gui.Size = UDim2.new(0,220,0,45)
    gui.StudsOffset = Vector3.new(0,3,0)
    gui.AlwaysOnTop = true
    gui.Adornee = cabe√ßa
    gui.Parent = head

    local txt = Instance.new("TextLabel")
    txt.Size = UDim2.new(1,0,1,0)
    txt.BackgroundTransparency = 1
    txt.TextStrokeTransparency = 0,4
    txt.Font = Enum.Font.SourceSansBold
    txt.TextSize = 14
    txt.Text = player.Name.." | "..player.AccountAge.." dias"
    txt.TextColor3 = getESPColor()
    txt.Parent = gui

    ESPObjects[player] = gui
fim

PlayerTab:AdicionarAltern√¢ncia({
    Nome = "ESP Ligado/Desligado",
    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        ESPEnabled = estado

        se estado ent√£o
            para _,p em ipairs(Players:GetPlayers()) fa√ßa
                CriarESP(p)
            fim

            tabela.inserir(ESPConnections, Players.PlayerAdded:Connect(function(p)
                p.CharacterAdded:Connect(function()
                    tarefa.esperar(0.4)
                    CriarESP(p)
                fim)
            fim))

            tabela.inserir(ESPConnections, Players.PlayerRemoving:Connect(function(p)
                RemoverESP(p)
            fim))

            tabela.inserir(ESPConnections, RunService.RenderStepped:Connect(function()
                se ESPColorMode == "RGB" ent√£o
                    para _,gui em pares(ESPObjects) fa√ßa
                        local txt = gui:FindFirstChildOfClass("TextLabel")
                        se txt ent√£o
                            txt.TextColor3 = getESPColor()
                        fim
                    fim
                fim
            fim))
        outro
            para _,gui em pares(ESPObjects) fa√ßa
                gui:Destruir()
            fim
            ESPObjects = {}

            para _,c em pares(ESPConnections) fa√ßa
                c:Desconectar()
            fim
            ESPConnections = {}
        fim
    fim
})



