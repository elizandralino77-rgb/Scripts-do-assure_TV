Aba de prote√ß√£o:AdicionarAltern√¢ncia({
    Nome = "Anti-Lag / Anti Tool",
    Padr√£o = falso,
    Callback = fun√ß√£o(estado)
        Se n√£o houver estado, retorne ao fim.
        tarefa.spawn(fun√ß√£o()
            local dedupLock = {}
            fun√ß√£o local isTool(inst) retorna inst:IsA("Tool") fim
            fun√ß√£o local reunir(jogador)
                itens locais = {}
                cont√™ineres locais = {}
                Se player.Character ent√£o table.insert(containers, player.Character) fim
                mochila local = jogador:EncontrarPrimeiroFilhoDaClasse("Mochila")
                se mochila ent√£o tabela.inserir(cont√™ineres, mochila) fim
                local sg = player:FindFirstChild("StarterGear")
                se sg ent√£o tabela.inserir(containers, sg) fim
                para _, container em ipairs(containers) fa√ßa
                    para _, filho em ipairs(container:GetChildren()) fa√ßa
                        se isTool(filho) ent√£o tabela.inserir(itens, filho) fim
                    fim
                fim
                devolver itens
            fim
            fun√ß√£o local dedupe(jogador)
                se dedupLock[jogador] ent√£o retorne fim
                dedupLock[player] = true
                ferramentas locais = reunir(jogador)
                se #ferramentas > 1 ent√£o
                    para i = 2, #ferramentas fa√ßa pcall(function() ferramentas[i]:Destroy() fim) fim
                fim
                dedupLock[player] = falso
            fim
            Jogadores.JogadorAdicionado:Conectar(fun√ß√£o(plr) deduplicar(plr) fim)
            para _,plr em ipairs(Players:GetPlayers()) fa√ßa dedupe(plr) fim
            enquanto o estado faz
                para _,plr em ipairs(Players:GetPlayers()) fa√ßa dedupe(plr) fim
                tarefa.esperar(2)
            fim
        fim)
    fim
})

