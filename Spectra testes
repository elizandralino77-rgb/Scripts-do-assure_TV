    canal.MensagemRecebida:Conectar(AoReceberMensagem)
fim)

--==============================
-- SPIN LOOP + TELEPORTE CAIXA
--==============================
RunService.RenderStepped:Connect(fun√ß√£o(dt)
    para _,_ em pares(SpinPlayers) fa√ßa
        local hrp=plr.Character e plr.Character:FindFirstChild("HumanoidRootPart")
        se hrp ent√£o hrp.CFrame*=CFrame.Angles(0,math.rad(1000*dt),0) fim
    fim
    para _, caixa em pares(CaixaPlayers) do
        local hrp=plr.Character e plr.Character:FindFirstChild("HumanoidRootPart")
        se hrp e caixa ent√£o
            local interno=falso
            para _, p em pares(caixa:GetChildren()) fa√ßa
                se p:IsA("BasePart") ent√£o
                    local s=p.Tamanho/2 local pos=p.Posi√ß√£o
                    local min=pos-s local max=pos+s
                    local pPos=hrp.Position
                    Se pPos.X>min.X e pPos.X<max.X e pPos.Y>min.Y e pPos.Y<max.Y e pPos.Z>min.Z e pPos.Z<max.Z ent√£o
                        dentro=verdadeiro quebrar
                    fim
                fim
            fim
            se n√£o estiver dentro, ent√£o hrp.CFrame=CFrame.new(hrp.Position) fim
        fim
    fim
fim)
