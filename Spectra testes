fun√ß√£o AntiFlingLoop(nome, localizador)
    fun√ß√£o de retorno(estado)
        tarefa.spawn(fun√ß√£o()
            enquanto o estado faz
                alvo local = localizador()
                se for alvo ent√£o
                    para _, parte em ipairs(target:GetDescendants()) fa√ßa
                        se parte:IsA("BasePart") ent√£o
                            parte.PodeColidir = verdadeiro
                            parte.Ancorada = verdadeiro
                        fim
                    fim
                fim
                tarefa.esperar(0.5)
            fim
        fim)
    fim
fim

