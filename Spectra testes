            local hrp = char e char:FindFirstChild("HumanoidRootPart")

            se n√£o (tRoot e tHead e hrp) ent√£o retorne fim

            -- ALTURA BASEADA NA SUA CINTURA
            local myHipHeight = hrp.Size.Y * 0.5

            -- ALTURA FINAL (BOCA + UM POUCO PRA CIMA)
