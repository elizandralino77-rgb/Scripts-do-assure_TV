fun√ß√£o local obterJogadores()
    local t = {}
    para _, p em ipairs(Players:GetPlayers()) fa√ßa
        se p ~= plr ent√£o
            tabela.inserir(t, p.Nome)
        fim
    fim
    retornar t
fim

